{
  "manifest_version": 2,
  "name": "Close Instagram Pics",
  "version": "0.1.1",
  "description": "Click the grey area around the instagram post to close the image.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": { "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": [
        "*://*.instagram.com/*"
      ],
      "js": ["js/instagram.js"]
    }
  ]
}